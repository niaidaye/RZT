                ul#stage.portfolio-4column
                  li(data-id='id-1', data-type='business')
                    .img-border-port
                      img.img-border.max-image(src='images/portfolio/product/video/3400.jpg', alt='')
                      .cover.boxcaption.box_2
                        .project-name
                          h5 广州南高铁工务段任务管理云平台
                        .project-text
                          a.textlink(href=':3400') 进入
                          a.textlink(href='images/portfolio/product/video/3400.jpg', data-gal='prettyPhoto[pp_gal_1]', title='Project title') 查看
                  li(data-id='id-2', data-type='wordpress')
                    .img-border-port
                      img.img-border.max-image(src='images/portfolio/product/video/3100.jpg', alt='')
                      .cover.boxcaption.box_2
                        .project-name
                          h5 RZT线路设备管理云平台
                        .project-text
                          a.textlink(href=':3100') 进入
